    "type": "symfony-bundle",
    "description": "Symfony DeruweSlugifyBundle",
    "keywords": ["slugify", "slug"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Deruwe",
            "email": "alexander.deruwe@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.0.14,<2.3-dev",
        "cocur/slugify": "0.1.*"
    },
    "autoload": {
        "psr-0": { "Deruwe\\SlugifyBundle": "" }
    },
    "target-dir": "Deruwe/SlugifyBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
